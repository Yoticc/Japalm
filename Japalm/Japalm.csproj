<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Yotic.Memory.Extensions" Version="1.1.3" />
  </ItemGroup>

  <ItemGroup>
    <Using Include=" System.Text" />
    <Using Include="Japalm.PointerUtils">
      <Static>True</Static>
    </Using>
    <Using Include="Memory" />
    <Using Include="System.Byte">
      <Alias>u1</Alias>
    </Using>
    <Using Include="System.UInt16">
      <Alias>u2</Alias>
    </Using>
    <Using Include="System.UInt32">
      <Alias>u4</Alias>
    </Using>
    <Using Include="System.UInt64">
      <Alias>u8</Alias>
    </Using>
  </ItemGroup>

</Project>
